@model IList<ProductOverviewModel>
@if (Model.Count > 0)
{
    <div class="related-products-grid product-grid">
        <div class="title">
            <strong>Use with</strong>
        </div>
        <div class="light-hr"></div>
        <div class="item-grid row justify-content-center">
            @foreach (var item in Model)
            {
                <div class="item-box col-xs-12 col-sm-5 col-lg-5 col-xxl-4 col-xxxl-3 px-3 pb-2 my-3">@await Html.PartialAsync("_ProductBox", item)</div>
            }
        </div>
    </div>
}